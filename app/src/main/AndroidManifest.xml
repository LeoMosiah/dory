<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dory">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Dory">
        <activity android:name=".ProfileActivity"></activity>
        <activity android:name=".QuizActivity" />
        <activity
            android:name=".StudyActivity"
            android:label="@string/title_activity_study"
            android:theme="@style/Theme.Dory.NoActionBar" />
        <activity android:name=".CreateUserActivity" />
        <activity android:name=".AuthenticationActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>